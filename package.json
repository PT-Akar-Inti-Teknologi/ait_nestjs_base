{
  "name": "nestjs-lib",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "npm run lint && npm run format && npm run build:clean && nest build && npm run copy:info",
    "build:clean": "node -e 'import(\"globby\").then(({globby})=>globby([\"./dist\", \"!./git\"],{onlyFiles:false}).then(p=>import(\"rimraf\").then(r => Promise.all(p.map(i=>r.sync(i))))))'",
    "copy:info": "node -e 'const pkg=require(\"./package.json\");delete pkg.scripts;delete pkg.devDependencies;pkg.main=\"Lib.js\";pkg.types=\"Lib.d.ts\";require(\"fs\").writeFileSync(\"./dist/package.json\", JSON.stringify(pkg))'",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "rimraf coverage && jest --coverage"
  },
  "dependencies": {
    "@google-cloud/storage": "6.12.0",
    "@nestjs/common": "9.4.3",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "9.4.3",
    "@nestjs/platform-express": "9.4.3",
    "@nestjs/typeorm": "10.0.0",
    "builder-pattern": "2.2.0",
    "reflect-metadata": "0.1.13",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.5.0",
    "@nestjs/testing": "9.4.3",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.12",
    "@types/supertest": "2.0.12",
    "@types/uuid": "9.0.2",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-plugin-prettier": "4.2.1",
    "globby": "13.2.2",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  },
  "jest": {
    "rootDir": "./",
    "roots": [
      "src",
      "test"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*\\Test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!test/**/*.(t|j)s",
      "!**/dto/**/*.(t|j)s",
      "!**/*Module.(t|j)s",
      "!**/Lib.(t|j)s"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "node"
  }
}
