{
  "name": "@ait/nestjs-base",
  "version": "0.0.1",
  "description": "Base module for NestJs boilerplate",
  "author": "ait",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "npm run lint && npm run format && npm run build:clean && nest build && npm run copy:info",
    "build:clean": "node -e 'import(\"globby\").then(({globby})=>globby([\"./dist\", \"!./git\"],{onlyFiles:false}).then(p=>import(\"rimraf\").then(r => Promise.all(p.map(i=>r.sync(i))))))'",
    "copy:info": "node -e 'const pkg=require(\"./package.json\");delete pkg.scripts;delete pkg.devDependencies;pkg.main=\"lib.js\";pkg.types=\"lib.d.ts\";require(\"fs\").writeFileSync(\"./dist/package.json\", JSON.stringify(pkg))'",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "rimraf coverage && jest --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.379.1",
    "@aws-sdk/s3-request-presigner": "3.379.1",
    "@google-cloud/storage": "6.12.0",
    "@nestjs/axios": "2.0.0",
    "@nestjs/common": "9.4.3",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "9.4.3",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/mapped-types": "1.2.2",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "9.4.3",
    "@nestjs/typeorm": "10.0.0",
    "axios": "1.4.0",
    "bcryptjs": "2.4.3",
    "builder-pattern": "2.2.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "lodash": "4.17.21",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.1.13",
    "uuid": "9.0.0",
    "typeorm-naming-strategies": "4.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@nestjs/cli": "9.5.0",
    "@nestjs/testing": "9.4.3",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.1",
    "@types/lodash": "4.14.197",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "18.16.12",
    "@types/passport-jwt": "3.0.9",
    "@types/supertest": "2.0.12",
    "@types/uuid": "9.0.2",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-plugin-prettier": "4.2.1",
    "globby": "13.2.2",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  },
  "jest": {
    "rootDir": "./",
    "roots": [
      "src",
      "test"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!test/**/*.(t|j)s",
      "!**/*.options.(t|j)s",
      "!**/*.dto.(t|j)s",
      "!**/*.module.(t|j)s",
      "!**/lib.(t|j)s"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "node"
  }
}
